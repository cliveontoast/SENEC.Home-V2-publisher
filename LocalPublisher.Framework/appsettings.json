{
  "CosmosDB": {
    "AccountEndPoint": "https://****.documents.azure.com:443/",
    "AccountKey": "****",
    "DefaultContainer": "SenecDev",
    "DatabaseName": "ToDoList"
  },
  "AllowedHosts": "*",
  "VoltSummary": {
    "MinutesPerSummary": 5
  },
  "EnergySummary": {
    "MinutesPerSummary": 5
  },
  "BatteryInverterTemperatureSummary": {
    "MinutesPerSummary": 5
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "LevelSwitches": {
      "$consoleSwitch": "Information",
      "$fileSwitch": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "levelSwitch": "$consoleSwitch"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day",
          "levelSwitch": "$fileSwitch"
        }
      }
    ]
  },
  "Timezone": "Australia/Perth",
  "FroniusIP": "",
  "TeslaPowerwall2IP": "192.168.91.1",
  "TeslaPowerwall2Password": "last five upper case letters of wifi password",
  "TeslaCredentialCacheSeconds":  480,
  "TeslaPowerwall2Email": "notused@example.com",
  "TeslaWriteFolder": "",
  "SenecIP": ""
}
