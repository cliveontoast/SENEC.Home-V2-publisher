{
  "CosmosDB": {
    "AccountEndPoint": "https://****.documents.azure.com:443/",
    "AccountKey": "****",
    "DefaultContainer": "SenecDev",
    "DatabaseName": "ToDoList"
  },
  "AllowedHosts": "*",
  "VoltSummary": {
    "MinutesPerSummary": 1
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "LevelSwitches": {
      "$consoleSwitch": "Debug",
      "$fileSwitch": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "levelSwitch": "$consoleSwitch"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day",
          "levelSwitch": "$fileSwitch"
        }
      }
    ]
  },
  "SenecIP": "192.168.0.199"
}
